/**
 *  @file cpmu.h
 *
 * 	@brief Contains Clock and power management unit API routines for MagniV devices.
 * 	
 *	@author Manuel Rodriguez
 *  
 */

#ifndef CPMU_H_
#define CPMU_H_

/* Typedefs */
/**
 * @typedef
 * */
typedef struct {
	unsigned char  syndiv;
	unsigned char  postdiv;
	unsigned char  refdiv;
}clock_dividers;

/* Prototypes */
void clock_init(unsigned long BusClock);
clock_dividers PLL(unsigned long FPLL);



#endif /* CPMU_H_ */
